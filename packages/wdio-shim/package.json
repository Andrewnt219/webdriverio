{
  "name": "@wdio/shim",
  "version": "7.13.0",
  "description": "A shim package to help translate between ESM and CommonJS",
  "author": "Christian Bromann <christian@saucelabs.com>",
  "homepage": "https://github.com/webdriverio/webdriverio/tree/main/packages/wdio-shim",
  "license": "MIT",
  "main": "./cjs/index.js",
  "type": "module",
  "types": "./build/index.d.ts",
  "module": "./build/index.js",
  "exports": {
    "./package.json": "./package.json",
    ".": [
      {
        "import": "./build/index.js",
        "require": "./cjs/index.js"
      },
      "./cjs/index.js"
    ]
  },
  "files": ["build"],
  "engines": {
    "node": ">=12"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/webdriverio/webdriverio.git"
  },
  "keywords": [
    "webdriver",
    "wdio"
  ],
  "bugs": {
    "url": "https://github.com/webdriverio/webdriverio/issues"
  },
  "publishConfig": {
    "access": "public"
  }
}
